{
  "name": "npmscripts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "delete": "rimraf dist",
    "build:pug": "pug --hierarchy -o dist/ src/pug/",
    "watch:pug": "pug --hierarchy -o dist/ src/pug/ --watch --pretty",
    "build:sass": "sass src/assets/scss/style.scss dist/assets/css/style.css --style compressed --no-source-map",
    "watch:sass": "sass src/assets/scss/style.scss dist/assets/css/style.css --style expanded -w",
    "autoprefixer": "postcss --use autoprefixer --no-map dist/assets/css/style.css -d dist/assets/css",
    "watch:autoprefixer": "onchange 'src/assets/scss/**/*.scss' -- yarn autoprefixer",
    "build:babel": "webpack --mode production",
    "watch:babel": "webpack --mode development --watch",
    "watch:imagemin": "onchange src/img/**/*.{jpg,png} -- node imagemin.mjs {{changed}}",
    "build:imagemin": "node imagemin.mjs",
    "server": "webpack-dev-server --watch-content-base --content-base dist/",
    "dev": "yarn watch:pug & yarn watch:sass & yarn watch:autoprefixer & yarn watch:babel & yarn server",
    "prd": "yarn delete && yarn build:pug && yarn build:sass && yarn autoprefixer && yarn build:babel && yarn build:imagemin"
  },
  "author": "techthreee",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "autoprefixer": "^9.7.1",
    "babel-loader": "^8.0.6",
    "imagemin": "^8.0.1",
    "imagemin-cli": "^5.1.0",
    "imagemin-keep-folder": "^5.3.2",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "jquery": "^3.6.0",
    "onchange": "^7.1.0",
    "postcss": "^7.0.21",
    "postcss-cli": "^6.1.3",
    "pug": "^2.0.4",
    "pug-cli": "github:pugjs/pug-cli#master",
    "rimraf": "^3.0.0",
    "sass": "^1.49.8",
    "three": "^0.137.5",
    "webpack": "^5.69.0",
    "webpack-cli": "^3",
    "webpack-dev-server": "^3.9.0"
  }
}
